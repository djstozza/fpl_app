<p id="notice"><%= notice %></p>

<h1>Fpl Teams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fpl_teams.each do |fpl_team| %>
      <tr>
        <td><%= link_to 'Show', fpl_team %></td>
        <td><%= link_to 'Edit', edit_fpl_team_path(fpl_team) %></td>
        <td><%= link_to 'Destroy', fpl_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fpl Team', new_fpl_team_path %>
