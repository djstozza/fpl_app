<p id="notice"><%= notice %></p>

<h1>List Positions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_positions.each do |list_position| %>
      <tr>
        <td><%= link_to 'Show', list_position %></td>
        <td><%= link_to 'Edit', edit_list_position_path(list_position) %></td>
        <td><%= link_to 'Destroy', list_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List Position', new_list_position_path %>
