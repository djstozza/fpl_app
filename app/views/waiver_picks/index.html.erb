<p id="notice"><%= notice %></p>

<h1>Waiver Picks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @waiver_picks.each do |waiver_pick| %>
      <tr>
        <td><%= link_to 'Show', waiver_pick %></td>
        <td><%= link_to 'Edit', edit_waiver_pick_path(waiver_pick) %></td>
        <td><%= link_to 'Destroy', waiver_pick, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Waiver Pick', new_waiver_pick_path %>
